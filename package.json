{
  "name": "create-wdk-module",
  "version": "1.0.0",
  "description": "Create WDK modules with a single command",
  "type": "module",
  "bin": {
    "create-wdk-module": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "lint": "ts-standard --ignore templates --ignore tests",
    "lint:fix": "ts-standard --ignore templates --ignore tests --fix",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "keywords": [
    "wdk",
    "wallet",
    "blockchain",
    "scaffold",
    "cli",
    "create",
    "tether"
  ],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetherto/create-wdk-module.git"
  },
  "bugs": {
    "url": "https://github.com/tetherto/create-wdk-module/issues"
  },
  "homepage": "https://github.com/tetherto/create-wdk-module#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "ora": "^8.1.1",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "validate-npm-package-name": "^6.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/prompts": "^2.4.9",
    "@types/validate-npm-package-name": "^4.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-standard": "^12.0.2",
    "typescript": "^5.7.2"
  }
}
