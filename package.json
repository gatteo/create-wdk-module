{
  "name": "@tetherto/create-wdk-module",
  "version": "1.0.0-beta.1",
  "description": "Create WDK modules with a single command",
  "type": "module",
  "bin": {
    "create-wdk-module": "./src/index.js"
  },
  "files": [
    "src",
    "templates"
  ],
  "scripts": {
    "start": "node src/index.js",
    "lint": "standard --ignore templates --ignore tests",
    "lint:fix": "standard --ignore templates --ignore tests --fix",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "prepublishOnly": "npm run lint && npm test"
  },
  "keywords": [
    "wdk",
    "wallet",
    "blockchain",
    "scaffold",
    "cli",
    "create",
    "tether"
  ],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetherto/create-wdk-module.git"
  },
  "bugs": {
    "url": "https://github.com/tetherto/create-wdk-module/issues"
  },
  "homepage": "https://github.com/tetherto/create-wdk-module#readme",
  "dependencies": {
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "ora": "^8.1.1",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "validate-npm-package-name": "^6.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "standard": "^17.1.2"
  },
  "standard": {
    "ignore": [
      "templates/**",
      "tests/**"
    ]
  }
}
